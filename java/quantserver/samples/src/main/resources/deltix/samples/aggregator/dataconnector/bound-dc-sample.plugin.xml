<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugIn>
  <type>DATA_CONNECTOR</type>
  
  <mainClass>deltix.samples.aggregator.dataconnector.SampleBoundDataConnector</mainClass>
  
  <title>Sample Connector (Bound API)</title>
  
  <description>
    Sample data connector, using the Bound API for sending messages.
  </description>

  <version>1</version>
  <minAPIVersion>1</minAPIVersion>

  <field>
    <key>host</key>
    <title>Host</title>
    <description>Host name or IP address of the data server</description>
    <type>HOST</type>   
    <defval>localhost</defval> 
  </field>
  
  <field>
    <key>port</key>
    <title>Port</title>
    <description>Port # of the data server</description>
    <type>PORT</type> 
    <defval>36994</defval>   
  </field>
</plugIn>
