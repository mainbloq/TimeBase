<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugIn>
  <type>DATA_CONNECTOR</type>
  
  <mainClass>deltix.samples.aggregator.dataconnector.TimeBasePlayer</mainClass>
  
  <title>TimeBase Player</title>
  
  <description>
    Sample data connector, which simulates a data feed by "playing" a 
    specified TimeBase stream at the speed at which it was recorded.
  </description>

  <version>1</version>
  <minAPIVersion>1</minAPIVersion>

  <field>
    <key>host</key>
    <title>Host</title>
    <description>Host name or IP address of TimeBase</description>
    <type>HOST</type>   
    <defval>localhost</defval> 
  </field>
  
  <field>
    <key>port</key>
    <title>Port</title>
    <description>Port # of the TimeBase</description>
    <type>PORT</type> 
    <defval>8011</defval>   
  </field>
  
  <field>
    <key>streamKey</key>
    <title>Stream</title>
    <description>TimeBase stream to play (the key)</description>
    <type>STRING</type>
    <allowedValue>.+</allowedValue>
    <defval></defval>   
  </field>
</plugIn>
